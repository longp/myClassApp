<link rel="stylesheet" type="text/css" href="/css/login.css">

<div class="container">
        <div class="card card-container">
            <!-- <img class="profile-img-card" src="//lh3.googleusercontent.com/-6V8xOA6M7BA/AAAAAAAAAAI/AAAAAAAAAAA/rzlHcD0KYwo/photo.jpg?sz=120" alt="" /> -->
            <img id="profile-img" class="profile-img-card" src="//ssl.gstatic.com/accounts/ui/avatar_2x.png" />
            <p id="profile-name" class="profile-name-card"></p>
            <form class="form-signin" method="post" action="/student_account">
                <span id="reauth-email" class="reauth-email"></span>
                <input type="text" name=studentUsername id="username" class="form-control" placeholder="Username" required autofocus>
                <input type="password" name=studentPassword id="inputPassword" class="form-control" placeholder="Password" required>
                <button class="btn btn-lg btn-primary btn-block btn-signin" type="submit">Sign in</button>
            </form><!-- /form -->
            <form method="get" action="/student_register" >
              <button class="btn btn-lg btn-danger btn-block btn-signin" type="submit">Register</button>
            </form>
        </div><!-- /card-container -->
    </div><!-- /container -->
